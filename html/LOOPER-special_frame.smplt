
@list<el><{ group_name <- el[1], SOURCE-LINK <- el[2], FRAME-ACTIONS <- el[3]}>
//
<@el>
<div id="@{group_name}_container" class="content-container" {{@{group_name}_styles}} >
    <iframe id="id-@{group_name}-frame" >>@{SOURCE-LINK} ? src="@{SOURCE-LINK}" : @nothing <<
            class="super-container >>@{FRAME-ACTIONS} ? @{FRAME-ACTIONS} : @nothing <<"  style="height:100%" {{iframe_@{group_name}_styles}} >
        @{group_name} frame
    </iframe>
</div>
//
</@el>

